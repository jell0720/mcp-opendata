# 新北市交通局 OpenData API 相關設定

# API 基本 URL
NTPC_OPENDATA_BASE_URL=https://data.ntpc.gov.tw/api/datasets

# API 認證金鑰（若需要）
NTPC_OPENDATA_API_KEY=your_api_key_here

# 請求超時設定（秒）
REQUEST_TIMEOUT=30

# 日誌級別設定 (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# MCP 服務設定
MCP_PORT=8000
MCP_HOST=127.0.0.1
